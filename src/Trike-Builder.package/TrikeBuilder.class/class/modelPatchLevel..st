modelPatchLevel: anInteger
	"Write TrikeModel class>>patchLevel."
	| method |

	"Don't change anything if nothing needs to be changed."
	TrikeModel patchLevel = anInteger ifTrue: [^self].

	method := String streamContents: [:stream |
		stream
			nextPutAll: 'patchLevel
	"Answer my patch level.  Automatically generated by TrikeBuilder class>>modelPatchLevel:."

	^';
			nextPutAll: anInteger printString; cr].
	TrikeModel class compile: method classified: 'versioning'.

	method := String streamContents: [:stream |
		stream
			nextPutAll: 'expectedModelPatchLevel
	"Answer the expected patch level of Trike''s model.  Automatically generated by TrikeBuilder class>>modelPatchLevel:."

	^';
			nextPutAll: anInteger printString; cr].
	TrikeNotebook class compile: method classified: 'versioning'
	